def find_first(line):

    D={'zero':0,
       'one':1,
       'two':2,
       'three':3,
       'four':4,
       'five':5,
       'six':6,
       'seven':7,
       'eight':8,
       'nine':9}

    for i in range(10):
        D[str(i)]=i
    idx=[]
    all_keys=[]
    for key in D:
        try:
            idx.append(line.index(key))
            all_keys.append(key)
        except ValueError:
            pass
         
    i=argmin(idx)
    digit=str(D[all_keys[i]])

    return digit


def find_last(line):

    D={'zero':0,
       'one':1,
       'two':2,
       'three':3,
       'four':4,
       'five':5,
       'six':6,
       'seven':7,
       'eight':8,
       'nine':9}

    line=line[::-1]  # reverse
    for i in range(10):
        D[str(i)]=i
    idx=[]
    all_keys=[]
    for key in D:
        try:
            idx.append(line.index(key[::-1]))
            all_keys.append(key)
        except ValueError:
            pass
         
    i=argmin(idx)
    digit=str(D[all_keys[i]])

    return digit


